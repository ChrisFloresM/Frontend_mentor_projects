round: var(--dark-desaturated-blue);\r\n$accent-transparent: var(--soft-violet-transparent);\r\n$accent: var(--soft-violet);\r\n$white-color: var(--white);\r\n$main-paragraph: var(--transparent-white-1);\r\n$stat-headings: var(--transparent-white-2);\r\n\r\n// ====================== Weigths ======================\r\n\r\n$bold-400: 400;\r\n$bold-700: 700;\r\n\r\n// ====================== font-size ====================\r\n\r\n$reset-10px: 62.5%;\r\n$base-15px: 1.5rem;\r\n\r\n// ===================== fonts ==========================\r\n$font-inter: 'Inter', 'sans-serif';\r\n$font-lexend: 'Lexend Deca', 'sans-serif';\r\n","sass\\abstracts\\_mixins.scss":"/* \r\nmedia queries breakpoints\r\n0    - 600px:  Phone\r\n600  - 900px:  tablet potrait\r\n900  - 1200px: tablet landscape\r\n1200 - 1800px: desktop\r\n1800+:         big desktop\r\n \r\n -phone\r\n -tab-port\r\n -tab-land\r\n -big-desktop\r\n*/\r\n@mixin respond($breakpoint) {\r\n  @if $breakpoint == phone {\r\n    @media (max-width: 37.5em) {\r\n      @content;\r\n    }\r\n  }\r\n  @if $breakpoint == tab-port {\r\n    @media (max-width: 56.25em) {\r\n      @content;\r\n    }\r\n  }\r\n  @if $breakpoint == tab-land {\r\n    @media (max-width: 75em) {\r\n      @content;\r\n    }\r\n  }\r\n  @if $breakpoint == big-desktop {\r\n    @media (min-width: 112.5em) {\r\n      @content;\r\n    }\r\n  }\r\n} ;\r\n","sass\\base\\_base.scss":"@use \"../abstracts\" as *;\r\n\r\nhtml {\r\n  font-size: $reset-10px;\r\n\r\n  @include respond(tab-land) {\r\n    font-size: 50%;\r\n  }\r\n\r\n  @include respond(phone) {\r\n    font-size: 40%;\r\n  }\r\n}\r\n\r\nbody {\r\n  font-family: $font-inter;\r\n  background-color: $main-background;\r\n}\r\n","sass\\layout\\_card.scss":"@use \"../abstracts\" as *;\r\n\r\n.main-card {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  height: 65%;\r\n  width: 75%;\r\n  background-color: $card-background;\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 50%);\r\n  grid-template-rows: repeat(3, 1fr);\r\n  color: $white-color;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n\r\n  @include respond(phone) {\r\n    grid-template-rows: 35% 65%;\r\n    height: 80%;\r\n    width: 90%;\r\n  }\r\n}\r\n","sass\\layout\\_card-info.scss":"@use \"../abstracts\" as *;\r\n\r\n.info {\r\n  grid-column: 1 / 2;\r\n  grid-row: 1 / -1;\r\n  padding: 4vw;\r\n  padding-top: 3vw;\r\n  padding-right: 5vw;\r\n\r\n  &__text {\r\n    margin-bottom: 5vw;\r\n  }\r\n\r\n  &__stats {\r\n    list-style: none;\r\n    display: flex;\r\n\r\n    &--stat:not(:last-child) {\r\n      margin-right: 6.5rem;\r\n\r\n      @include respond(tab-port) {\r\n        margin-right: 0;\r\n        margin-bottom: 3.5rem;\r\n      }\r\n    }\r\n\r\n    @include respond(tab-port) {\r\n      flex-direction: column;\r\n      justify-content: center;\r\n      align-items: center;\r\n    }\r\n  }\r\n\r\n  @include respond(tab-port) {\r\n    text-align: center;\r\n  }\r\n\r\n  @include respond(phone) {\r\n    grid-row: 2 / 3;\r\n    grid-column: 1 / -1;\r\n  }\r\n}\r\n\r\n.stat_number {\r\n  display: block;\r\n  margin-bottom: 0.5rem;\r\n}\r\n","sass\\layout\\_card-img.scss":"@use \"../abstracts\" as *;\r\n\r\n.img {\r\n  background-image: linear-gradient(to right bottom, $accent-transparent, $accent-transparent),\r\n    url(../assets/img/image-header-desktop.jpg);\r\n  background-size: cover;\r\n  grid-row: 1 / -1;\r\n  grid-column: 2 / 3;\r\n\r\n  @include respond(phone) {\r\n    grid-row: 1 / 2;\r\n    grid-column: 1 / -1;\r\n  }\r\n}\r\n","sass\\base\\_typography.scss":"@use \"../abstracts\" as *;\r\n\r\n// ======================= info - heading =================\r\n.info__heading {\r\n  font-size: 3.5rem;\r\n  font-weight: $bold-700;\r\n}\r\n\r\n.highlight {\r\n  color: $accent;\r\n}\r\n// ========================= info - paragraph ===============\r\n.info__text {\r\n  font-size: 1.5rem;\r\n  line-height: 1.5;\r\n  color: $main-paragraph;\r\n}\r\n\r\n// ========================== info - stats ==================\r\n\r\n.info__stats {\r\n  &--stat {\r\n    font-size: 1.2rem;\r\n    text-transform: uppercase;\r\n    color: $stat-headings;\r\n  }\r\n}\r\n.stat_number {\r\n  font-size: 2rem;\r\n  font-weight: $bold-700;\r\n  color: $white-color;\r\n}\r\n","sass\\main.scss":"/* \r\n# Front-end Style Guide\r\n\r\n## Layout\r\n\r\nThe designs were created to the following widths:\r\n\r\n- Mobile: 375px\r\n- Desktop: 1440px\r\n*/\r\n\r\n@use \"base/base\";\r\n@use \"layout/card\";\r\n@use \"layout/card-info\";\r\n@use \"layout/card-img\";\r\n@use \"base/typography\";\r\n"},"lineCount":null}},"error":null,"hash":"3ec8ec64b048a495aa7dfab302717d05","cacheData":{"env":{}}}